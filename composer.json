{
    "name": "dizzy7/doctrine-dateinterval",
    "description": "Supports DateInterval in Doctrine DBAL and ORM.",
    "keywords": ["doctrine", "date", "interval"],
    "homepage": "http://herrera-io.github.com/php-doctrine-dateinterval",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        },
        {
            "name": "Evgeniy Zatsepin",
            "email": "7dizzy7@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dizzy7/php-doctrine-dateinterval/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/dbal": "~2.4",
        "herrera-io/date-interval": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "doctrine/orm": "~2.4"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Doctrine": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
